<odoo>
	<data>

		<record id="view_master_subruang_tree" model="ir.ui.view">
			<field name="name">master.subruang.tree</field>
			<field name="model">master.subruang</field>
			<field name="type">tree</field>
			<field name="priority" eval="8"/>
			
			<field name="arch" type="xml">
				<tree string="Sub Ruang">
					<field name="ruang_id" />
					<field name="no_subruang" />
					<field name="nama_subruang" />
				</tree>
			</field>
		</record>

		<record id="view_master_subruang_form" model="ir.ui.view">
			<field name="name">master.subruang.form</field>
			<field name="model">master.subruang</field>
			<field name="type">form</field>
			<field name="priority" eval="8"/>

			<field name="arch" type="xml">
				<form string="Sub Ruang Form" version="7.0">
					<sheet>
						<group>
							<field name="ruang_id"/>
							<field name="no_subruang"/>
							<field name="nama_subruang"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>

	</data>
</odoo>