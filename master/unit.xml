<odoo>
	<data>
		
		<record id="view_master_unit_tree" model="ir.ui.view">
			<field name="name">master.unit.tree</field>
			<field name="model">master.unit</field>
			<field name="type">tree</field>
			<field name="priority" eval="8"/>
			
			<field name="arch" type="xml">
				<tree string="Unit">
					<field name="no_unit" />
					<field name="name" />
				</tree>
			</field>
		</record>

		<record id="view_master_unit_form" model="ir.ui.view">
			<field name="name">master.unit.form</field>
			<field name="model">master.unit</field>
			<field name="type">form</field>
			<field name="priority" eval="8"/>

			<field name="arch" type="xml">
				<form string="Unit Form" version="7.0">
					<sheet>
						<div class="oe_title">
							<label for="name" class="oe_edit_only" string="Nama Unit"/>
							<h1>
								<field name="name"/>
							</h1>
						</div>
						<group>
							<group>
								<field name="no_unit"/>
							</group>
						</group>
					</sheet>
				</form>
			</field>
		</record>

	</data>
</odoo>